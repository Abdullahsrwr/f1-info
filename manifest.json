{
    "manifest_version": 3,
    "name": "F1 Info",
    "version": "1.0",
    "description": "Displays F1 race Wekend Info.",
    "permissions": ["http://ergast.com/api/*", "activeTab"],
    "action": {
      "default_popup": "f1.html",
      "default_icon": {
        "16": "images/f1.png",
        "48": "images/f1.png",
        "128": "images/f1.png"
      }
    },
    "icons": {
      "16": "images/f1.png",
      "48": "images/f1.png",
      "128": "images/f1.png"
    },
    "browser_action": {
      "default_popup": "f1.html",
      "default_icon": {
        "16": "images/f1.png",
        "48": "images/f1.png",
        "128": "images/f1.png"
      }
    }
  }
  